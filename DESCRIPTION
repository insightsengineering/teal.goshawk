Package: teal.goshawk
Type: Package
Title: Longitudinal visualization teal modules
Date: 2019-12-20
Version: 0.1.1
Authors@R: c(
  person("Nick", "Paszty", email = "nick.paszty@gene.com", role = c("aut", "cre")),
  person("Wenyi", "Liu", email = "liu.wenyi@roche.com", role = c("aut")),
  person("Jeffrey", "Tomlinson", email = "tomlinson.jeffrey@roche.com", role = c("aut")),
